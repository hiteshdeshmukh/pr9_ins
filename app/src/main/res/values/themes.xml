<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.Pr9_ins" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/purple_500</item>
        <item name="colorPrimaryVariant">@color/purple_700</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
    </style>

    <!-- Light Mode -->
    <!-- Screen Background if light mode is on -->
    <style name="Screen">
        <item name="android:background">#000000</item>
    </style>

    <!-- Text Background if light mode is on -->
    <style name="TextColor">
        <item name="android:textColor">#000000</item>
    </style>

    <!-- button Background if light mode is on -->
    <style name="ButtonBackground1">
        <item name="android:backgroundTint">#B9BABA</item>
        <item name="android:textColor">#000000</item>
    </style>
    <style name="ButtonBackground2">
        <item name="android:backgroundTint">#FFFFFF</item>
        <item name="android:textColor">#000000</item>
    </style>

    <style name="IconBackground">
        <item name="android:background">@android:color/transparent</item>
    </style>

    <style name="BottomNavBackground">
        <item name="android:background">?android:attr/windowBackground</item>
    </style>
  <!-- Base application theme. -->
    <style name="Base.Theme.Pr9_ins" parent="Theme.Material3.DayNight.NoActionBar">
        <!-- Customize your light theme here. -->
        <!-- <item name="colorPrimary">@color/my_light_primary</item> -->
    </style>


    <style name="DialogAnimation1">
        <item name="android:windowEnterAnimation">@anim/slide_in_bottom</item>
        <item name="android:windowExitAnimation">@anim/slide_out_bottom</item>
    </style>



</resources>